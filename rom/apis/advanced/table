loader.includeOriginalApi()

function clone(table)
  local copy = {}
  for k, v in pairs(table) do
    copy[k] = v
  end
  return copy
end

function dump(table)
  for k, v in pairs(table) do
    print(k, v)
  end
end

function copyNoOverwrite(src, dst)
  for k, v in pairs(src) do
    if nil==dst[k] then
      dst[k] = v
    end
  end
end
